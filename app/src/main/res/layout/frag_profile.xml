            <!--/>-->


    </RelativeLayout>

    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
        android:id="@+id/swipeRefreshProfile"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="60dp">

    <LinearLayout
        android:background="@color/colorWhiteE"

        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">




                <LinearLayout

           android:layout_marginTop="20dp"
                android:layout_gravity="center_horizontal"
                    android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">


                <androidx.cardview.widget.CardView


                    android:id="@+id/cardView"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:shape="ring"
                    app:cardBackgroundColor="@color/colorPrimaryDark"
                    app:cardCornerRadius="50dp">

                    <androidx.cardview.widget.CardView


                        android:id="@+id/cardViewWhiteBG"
                        android:layout_gravity="center"
                        android:layout_width="96dp"
                        android:layout_height="96dp"
                        android:shape="ring"
                        app:cardBackgroundColor="#ffffff"
                        app:cardCornerRadius="48dp">


                        <androidx.cardview.widget.CardView
                            android:layout_width="90dp"
                            android:layout_height="90dp"
                            android:layout_gravity="center"
                            android:shape="ring"
                            app:cardCornerRadius="45dp">

                            <ImageView
                                android:layout_gravity="center"
                                android:id="@+id/imgUserImage"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:adjustViewBounds="true"
                                android:scaleType="centerCrop"
                                android:src="@drawable/profileplchlder"
                                android:visibility="visible" />







                        </androidx.cardview.widget.CardView>


                    </androidx.cardview.widget.CardView>




                </androidx.cardview.widget.CardView>


            </LinearLayout>





                <TextView
                    android:id="@+id/profileName"
                    android:gravity="center"

android:layout_marginTop="10dp"
                    android:layout_gravity="center"
                    android:textColor="@color/colorPrimaryDark"
                    android:fontFamily="@font/proximanova_semibold"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text=" "
                    android:textSize="18dp"/>


                <TextView
                    android:id="@+id/profileUserName"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_marginTop="8dp"



                    android:fontFamily="@font/proximanova_regular"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text=" "
                    android:textSize="16dp"/>

                <TextView
                    android:id="@+id/profileEmail"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_marginTop="10dp"

android:visibility="gone"

                    android:fontFamily="@font/proximanova_regular"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text=" "
                    android:textSize="16dp"/>




                <View
                    android:background="@color/colorgray"
                    android:layout_marginTop="10dp"
                    android:id="@+id/view"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_weight="1" />


                <LinearLayout
                    android:id="@+id/savedlinear"
                    android:layout_marginTop="10dp"
                    android:weightSum="1"

                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:orientation="horizontal">


                    <ImageView
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"



                        android:layout_width="0dp"
                        android:layout_height="22.5dp"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="0.2"
                        app:srcCompat="@drawable/homedown" />



                    <TextView
                        android:fontFamily="@font/proximanova_regular"

                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginLeft="15dp"
                        android:layout_weight="0.6"
                        android:gravity="center_vertical"
                        android:text="Saved Locations"
                        android:textSize="16dp" />

                    <ImageView
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"

                        android:id="@+id/imageView2"

                        android:layout_width="0dp"
                        android:layout_height="22.5dp"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="0.2"
                        app:srcCompat="@drawable/grayarrow" />
                </LinearLayout>

                <View
                    android:background="@color/colorgray"
                    android:layout_marginTop="4dp"
                    android:id="@+id/view2"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_weight="1" />



                <LinearLayout
                    android:id="@+id/sharedlinear"
                    android:layout_marginTop="10dp"

                    android:weightSum="1"

                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:orientation="horizontal">


                    <ImageView
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"



                        android:layout_width="0dp"
                        android:layout_height="22.5dp"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="0.2"
                        app:srcCompat="@drawable/groupnew" />

